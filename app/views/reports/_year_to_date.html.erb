<div class="report-section">
  <h2 class="border-bottom">Year to date</h2>
  <div id='chart-yearly_report'></div>
  <div class="row">
    <% lines.each do |line| %>
      <div class="col-sm-4">
        <h3><%= link_to line, '#', class: 'line-header' %></h3>
        <p>Patients contacted: <strong><%= @yearly_report[line.to_sym][:patients_contacted] %></strong></p>
        <p>New patients contacted: <strong><%= @yearly_report[line.to_sym][:new_patients_contacted] %></strong></p>
        <p>Pledges sent: <strong><%= @yearly_report[line.to_sym][:pledges_sent] %></strong></p>
      </div>
   <% end %>
  </div>
</div>
