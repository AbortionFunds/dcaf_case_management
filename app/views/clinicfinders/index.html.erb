<h1>Clinic Finder</h1>

<h3>Patient information</h3>

<div class="col-sm-4">
  <%= bootstrap_form_tag url: '/clinicfinder', remote: true do |f| %>
    <%= f.number_field :zip, label: 'Zip Code', autofocus: true %>
    <%= f.number_field :gestation, label: 'Gestational Age' %>
    <%= f.submit 'Find Clinics', class: 'btn btn-primary' %>
  <% end %>
</div>

<div class="clinic-finder-results"></div>
